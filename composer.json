{
  "name": "meta4creations/post-duplicator",
  "description": "Creates functionality to duplicate any and all post types, including taxonomies & custom fields.",
  "type": "wordpress-plugin",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "metaphorcreations/mtphr-settings": "v1.1.4"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:metaphorcreations/mtphr-settings.git"
    }
  ],
  "scripts": {
    "post-install-cmd": [
      "@copy-mtphr-settings"
    ],
    "post-update-cmd": [
      "@copy-mtphr-settings"
    ],
    "copy-mtphr-settings": [
      "php scripts/copy-mtphr-settings.php"
    ]
  }
}
